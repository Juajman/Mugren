; Definition file for The Shockmaster (Vinetrigger Edition)
; Contains all the filenames needed for the character

; Player information
[Info]
name = "The Shockmaster"        ;Name of character
displayname = "The Shockmaster" ;Name of character to display
versiondate = 05,02,2019    ;Version date of character (MM-DD-YYYY or X.XX) ;Production Started on February 24th, 2018
mugenversion = 1.1          ;Version of M.U.G.E.N character works on (X.XX)
author = "Brergrsart"       ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240        ;Local coordinate space width and height
;SC;Idle=1813
;SC;Preview=185,8

; Files for the player
[Files]
cmd     = TheShockmaster.cmd           ;Command set
cns     = TheShockmaster.cns           ;Constants
st      = TheShockmaster.cns           ;States
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = TheShockmaster.sff           ;Sprite
anim    = TheShockmaster.air           ;Animation
sound   = TheShockmaster.snd           ;Sound
ai      = TheShockmaster.ai            ;AI hints data (not used)

; Maps character selection buttons to palette numbers.
[Palette Keymap]
a = 1 ;Press button X to select palette 1, etc.
b = 2
c = 3
x = 4
y = 5
x = 6
;a2 = 7 ;Hold Start and press button X for palette 7, etc.
;b2 = 8
;c2 = 9
;x2 = 10
;y2 = 11
;z2 = 12

; Arcade mode
[Arcade]
intro.storyboard = intro.def
ending.storyboard = ending.def
